{
  "name": "@gratiaos/icons",
  "version": "1.0.1",
  "funding": "https://github.com/sponsors/GratiaOS",
  "type": "module",
  "license": "AGPL-3.0-only",
  "description": "Composable theme-friendly SVG icons for Garden apps.",
  "publishConfig": {
